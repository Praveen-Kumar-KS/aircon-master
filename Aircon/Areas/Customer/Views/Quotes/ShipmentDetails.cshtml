@using Aircon.Areas.Customer.Models.ShipmentInformation

@model ShipmentDetailViewModel

<div class="card m-t-20">
    <div class="row">
        <div class="col-xl-6 col-lg-6 col-md-8 col-sm-12 col-12"><h6 class="card-header bold">Shipment Details</h6></div>
    </div>
  
        <div class="card-body p-t-0">
            <div class="row">
                <div class="col-xl-3 col-lg-4 col-md-2 col-sm-12 col-12 ">
                    <air-input asp-for="@Model.Quantity"></air-input>
                </div>
                <div class="col-xl-3 col-lg-4 col-md-2 col-sm-12 col-12 ">
                    <air-input asp-for="@Model.Weight"></air-input>
                </div>
                <div class="col-xl-3 col-lg-4 col-md-2 col-sm-12 col-12 ">
                    <air-input asp-for="@Model.Volume"></air-input>
                </div>
            </div>
        </div>
    
    </div>