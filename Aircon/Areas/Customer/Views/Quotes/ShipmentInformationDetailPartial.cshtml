@using Aircon.Areas.Customer.Models.ShipmentInformation

@model ShipmentInformationDetailViewModel
<div>
    
        <div class="modal-header green-top">
            <h6 class=" subtitle1 p-0 bold">Add ShipmentDetail</h6>
            <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                <i class="m-r-10 mdi txt-black mdi-close"></i>
            </button>
        </div>

        <div class="modal-body">
            <div class=" subtitle1 p-0 normal-font m-b-10">ShipmentInformationDetail</div>
            <div class="row">
                <div class="col-xl-6 col-lg-6 col-md-6 col-sm-12 col-12 m-t-20">
                    <air-input asp-for="@Model.Quantity"></air-input>
                </div>
                <div class="col-xl-6 col-lg-6 col-md-6 col-sm-12 col-12 m-t-20">
                    <air-input asp-for="@Model.Volume"></air-input>
                </div>
                <div class="col-xl-6 col-lg-6 col-md-6 col-sm-12 col-12 m-t-20">
                    <air-input asp-for="@Model.Weight"></air-input>
                </div>
                <div class="col-xl-6 col-lg-6 col-md-6 col-sm-12 col-12 m-t-20">
                    <air-input asp-for="@Model.Width"></air-input>
                </div>
                <div class="col-xl-6 col-lg-6 col-md-6 col-sm-12 col-12 m-t-20">
                    <air-input asp-for="@Model.Height"></air-input>
                </div>
   
            </div>

 

        </div>

        <div class="modal-footer">
            <div class="col-md-12 pb-30">
                <button class="btn float-right btn-primary" data-dismiss="modal">DISCARD CHANGES</button>
                <button type="button" class="btn float-right btn-secondary">SAVE</button>
            </div>
        </div>



</div>